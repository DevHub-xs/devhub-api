{
  "clusterName": "devhub-prod-cluster",
  "providerSettings": {
    "providerName": "AWS",
    "regionName": "US_EAST_1",
    "instanceSizeName": "M30"
  },
  "replicationSpecs": [{
    "numShards": 1,
    "regionsConfig": {
      "US_EAST_1": {
        "analyticsNodes": 1,
        "electableNodes": 3,
        "priority": 7,
        "readOnlyNodes": 1
      }
    }
  }],
  "backupEnabled": true,
  "pitEnabled": true,
  "biConnector": {
    "enabled": false
  },
  "clusterType": "REPLICASET",
  "diskSizeGB": 40,
  "mongoDBMajorVersion": "7.0",
  "autoScaling": {
    "diskGBEnabled": true,
    "compute": {
      "enabled": true,
      "scaleDownEnabled": true
    }
  }
}
